{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
    "routes": [
        {
            "src": "/api/index.php",
            "dest": "/api/index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "public": true
}