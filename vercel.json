{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)\\.php",
      "dest": "/api/$1.php"
    },
    {
      "src": "/resource/(.*)",
      "dest": "/public/resource/$1"
    },
    {
      "src": "/font/(.*)",
      "dest": "/public/font/$1"
    },
    {
      "src": "/webfonts/(.*)",
      "dest": "/public/font/fontawesome-free-6.7.2-web/webfonts/$1"
    },
    {
      "src": "/(.*)\\.css",
      "dest": "/public/resource/style/$1.css"
    },
    {
      "src": "/(.*)\\.js",
      "dest": "/public/resource/script/$1.js"
    },
    {
      "src": "/logo/(.*)\\.(png|jpg|jpeg|gif|svg|ico)",
      "dest": "/public/resource/src/logo/$1"
    },
    {
      "src": "/src/(.*)\\.(png|jpg|jpeg|gif|svg|ico)",
      "dest": "/public/resource/src/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
